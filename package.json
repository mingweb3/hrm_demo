{
  "name": "reactjs-starter-with-vite",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "build-prod": "npm run build && npm run preview",
    "clean": "rimraf yarn.lock package-lock.json node_modules build",
    "check-types": "tsc --noEmit --pretty",
    "lint": "eslint . \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint . \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "prettier:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json}\"",
    "prettier:write": "prettier --write \"**/*.{js,jsx,ts,tsx,json}\"",
    "prepare": "husky install"
  },
  "babelMacros": {
    "twin": {
      "preset": "styled-components"
    }
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@mui/base": "^5.0.0-alpha.114",
    "@mui/material": "^5.11.7",
    "@mui/system": "^5.11.5",
    "@mui/x-date-pickers": "^6.3.0",
    "@reduxjs/toolkit": "^1.9.0",
    "@tanstack/react-query": "^4.29.5",
    "@tanstack/react-table": "^8.7.9",
    "@uiw/react-md-editor": "^3.20.5",
    "axios": "^1.3.6",
    "clsx": "^1.2.1",
    "dayjs": "^1.11.7",
    "i18next": "^22.0.6",
    "i18next-browser-languagedetector": "^7.0.1",
    "lodash": "^4.17.21",
    "notistack": "^3.0.1",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-app-polyfill": "^3.0.0",
    "react-currency-input-field": "^3.6.10",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-google-autocomplete": "^2.7.3",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.43.9",
    "react-i18next": "^12.0.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.4.3",
    "redux-injectors": "^2.1.0",
    "redux-saga": "^1.2.1",
    "use-mask-input": "^3.1.2"
  },
  "devDependencies": {
    "@pancakeswap/eslint-config-pancake": "1.2.0",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.9",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@vitejs/plugin-react": "^2.2.0",
    "autoprefixer": "^10.4.13",
    "cssnano": "^5.1.14",
    "eslint": "^7.2.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.21",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.6.4",
    "vite": "^3.2.3"
  }
